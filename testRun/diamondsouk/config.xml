<?xml version='1.0' encoding='utf-8'?>
<widget id="com.diamondsouk" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
  <name>Diamondsouk</name>
  <description>
    Diamond Souk.
  </description>
  <author email="support@diamondsouk.ae" href="">
    Diamond Souk Team
  </author>
  <content src="index.html" />
  <preference name="DisallowOverscroll" value="true" />
  <preference name="android-minSdkVersion" value="14" />
  <preference name="StatusBarOverlaysWebView" value="false" />
  <preference name="StatusBarBackgroundColor" value="#ee6e73" />
  <preference name="StatusBarStyle" value="blacktranslucent" />

  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />

